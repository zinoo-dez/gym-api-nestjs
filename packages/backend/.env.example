DATABASE_URL="postgresql://username:password@localhost:5432/gym_api"
APP_ENV="development"
JWT_SECRET="your-secret-key"
PORT=3000
CORS_ORIGINS="http://localhost:5173,http://localhost:3000,http://localhost:3001"

# Logging Configuration
# Log level: debug, info, warn, error
LOG_LEVEL=debug

# Log format: json, pretty
LOG_FORMAT=pretty

# Enable/disable colors in console output
LOG_COLORS=true

# Service name for log entries
SERVICE_NAME=gym-api

# Service version
SERVICE_VERSION=1.0.0

# Comma-separated list of additional sensitive fields
SENSITIVE_FIELDS=
